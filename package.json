{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require dotenv/config ./build/server.js",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@graphql-tools/schema": "^9.0.17",
    "@remix-run/express": "^1.14.3",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@twurple/api": "^6.0.9",
    "@twurple/auth": "^6.0.9",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.12.0",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "pino-http": "^8.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/ws": "^8.5.4",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.16",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=18"
  }
}
